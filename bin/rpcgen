#!/usr/bin/env python

import sys
import os
sys.path += os.path.abspath(os.path.join(os.path.split(__file__)[0], "../pylib")),
from simplerpc.rpcgen import rpcgen

if __name__ == "__main__":
    if len(sys.argv) < 2:
        sys.stdout.write("usage: %s <rpc-source-file>\n" % sys.argv[0])
        exit(1)
    rpcgen(sys.argv[1])
